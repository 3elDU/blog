{
  "name": "blog",
  "type": "module",
  "private": true,
  "main": "index.js",
  "author": "petafloppa",
  "scripts": {
    "dev": "concurrently 'eleventy --watch' 'wrangler dev -c workers/blog/wrangler.jsonc'",
    "build": "eleventy",
    "types": "wrangler types --cwd workers/blog && wrangler types --cwd workers/redir",
    "deploy": "wrangler deploy --c workers/blog/wrangler.jsonc && wrangler deploy -c workers/redir/wrangler.jsonc"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "concurrently": "^9.2.0",
    "esbuild": "^0.25.6",
    "lightningcss": "^1.29.1",
    "typescript": "^5.8.3",
    "wrangler": "^4.24.0"
  },
  "dependencies": {
    "cookie": "^1.0.2",
    "hono": "^4.8"
  }
}
